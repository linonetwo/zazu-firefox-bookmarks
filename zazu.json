{
  "name": "zazu-firefox-bookmarks-history-search",
  "version": "2.0.0",
  "description": "Firefox bookmark and history searcher for Zazu.",
  "icon": "assets/firefox.png",
  "blocks": {
    "input": [
      {
        "id": "Firefox Bookmarks",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "b",
        "script": "src/scripts/searchBookmark.js",
        "connections": ["Open"]
      },
      {
        "id": "Firefox History",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "h",
        "script": "src/scripts/searchHistory.js",
        "connections": ["Open"]
      },
      {
        "id": "Firefox ",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "f",
        "script": "src/scripts/searchAll.js",
        "connections": ["Open"]
      }
    ],
    "output": [
      {
        "id": "Open",
        "type": "OpenInBrowser",
        "url": "{value}"
      }
    ]
  }
}
